{
  "name": "leap25-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "node build.js",
    "start": "node dist/index.js",
    "dev": "nodemon --ext ts --watch \"./**/*.ts\" --exec \"node build.js && node dist/index.js\"",
    "clean": "rimraf dist",
    "test": "vitest run --config ./vitest.config.ts",
    "test:watch": "vitest --config ./vitest.config.ts",
    "test:coverage": "vitest run --coverage --config ./vitest.config.ts",
    "test:unit": "vitest run tests/unit --config ./vitest.config.ts",
    "test:integration": "vitest run tests/integration --config ./vitest.config.ts",
    "test:skip-integration": "vitest run --config ./vitest.config.ts --exclude 'tests/integration/**/*.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "vitest related --run --config ./vitest.config.ts --dir tests/unit"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlsu-lscs/leap25-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dlsu-lscs/leap25-backend/issues"
  },
  "homepage": "https://github.com/dlsu-lscs/leap25-backend#readme",
  "dependencies": {
    "axios": "^1.9.0",
    "connect-redis": "^8.0.3",
    "contentful": "^11.5.18",
    "contentful-management": "^11.52.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.15.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^3.14.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "punycode": "^2.3.1",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.2",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
